<body>
    <div id="bg-container">
        <div id="particles-js"></div>
    </div>

    <header id="start-page" class="full-screen-page">
        <div id="start-page-bg" data-aos="fade-down">
        </div>
        <img src="dist/img/headerText.png" alt="" class="title-header-text" data-aos="fade-in">
        <h3 id="start-msg-en">
            Happy Birthday!!
        </h3>
        <div id="scroll-arrow-hint">
            <span></span>
            <span></span>
            <span></span>
        </div>
    </header>

    <section id="artbook">
        <div class="section-title" data-aos="fade-in">
            <h1>Art (Click to enlarge)</h1>
            <h3 lang="ja">画集（写真をクリックすると拡大します）</h3>
        </div>
        <div id="artbook-gallery">
            {{#each artbook_data}}
            <a href="{{fullpage}}" data-aos="fade-up">
                <img class="artbook-page" srcset="{{thumbnail_small}} 150w, {{thumbnail}} 500w"
                    sizes="(max-width: 700px) 150px, 500px" src="{{thumbnail}}" alt="" />
                <div style="display: none;" class="art-caption-container">
                    <div class="art-caption">
                        <div class="artist-details">
                            <div class="caption-text">
                                <!-- htmlmin:ignore -->{{caption_text}}
                                <!-- htmlmin:ignore -->
                            </div>
                            <div class="artist-name">
                                <div>
                                    {{artist}}
                                </div>
                                {{#if artist_link}}
                                <div>{{artist_link}}</div>
                                {{/if}}
                            </div>
                        </div>
                        <div class="artist-nation-flag">
                            {{#if country_code}}
                                <span class="flag-icon flag-icon-{{country_code}}" title="{{country_name}}"
                                    alt="{{country_name}}"></span>
                            {{/if}}
                        </div>
                    </div>
                </div>
            </a>
            {{/each}}
        </div>
    </section>

