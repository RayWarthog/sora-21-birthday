    <section id="birthday-msgs">
        <div class="section-title" data-aos="fade-in">
            <h1>Messages</h1>
            <h3 lang="ja">寄せ書きメッセージ</h3>
        </div>
        <div class="card-columns">
            {{#each messages}}
            <div class="birthday-msg-card-container" data-aos="fade-in">
                <div class="birthday-msg-card card card-default">
                    <div class="sender-details">
                        <div>
                            <img class="ankimo-img" src="dist/img/sddefault_live.png" alt=""></img>
                        </div>
                        <div class="sender-name-container">
                            <span class="sender-name">{{username}}</span>
                            {{#with twitter}}
                            <span class="twitter-name text-muted">@{{this}}</span>
                            {{/with}}
                        </div>
                        <div>
                            {{#if country_code}}
                            <span class="flag-icon flag-icon-{{country_code}}" title="{{country_name}}"
                                alt="{{country_name}}"></span>
                            {{/if}}
                        </div>
                        <div>
                            <img class="shooting-star-img" src="dist/img/shootingStar.png" alt=""></img>
                        </div>
                    </div>
                    <div class="birthday-msg">
                        <blockquote class="blockquote mb-0" {{#if isMsgInJP}}lang="ja" {{/if}}>
                            <!-- htmlmin:ignore -->
                            <p>{{message}}</p>
                            <!-- htmlmin:ignore -->
                        </blockquote>
                        {{#if message_jp}}
                        <hr>
                        <blockquote class="blockquote mb-0" lang="ja">
                            <!-- htmlmin:ignore -->
                            <p>{{message_jp}}</p>
                            <!-- htmlmin:ignore -->
                        </blockquote>
                        {{/if}}
                    </div>
                </div>
            </div>
            {{/each}}
        </div>
    </section>

    <section id="end-page" class="half-screen-page">
